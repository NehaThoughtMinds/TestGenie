{
  "name": "testgenie",
  "displayName": "testgenie",
  "description": "Unit Test Case Generator",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "testgenie.generate",
        "title": "TestGenie: Generate Unit Tests for This File"
      },
      {
        "command": "testgenie.generateFromJira",
        "title": "TestGenie: Generate from Jira Story"
      }
    ],
    "configuration": {
      "title": "TestGenie",
      "properties": {
        "testgenie.openaiApiKey": {
          "type": "string",
          "default": "",
          "description": "Your OpenAI API key"
        },
        "testgenie.openaiModel": {
          "type": "string",
          "default": "gpt-4o",
          "enum": [
            "gpt-4o",
            "gpt-4-turbo",
            "gpt-3.5-turbo"
          ],
          "description": "OpenAI model to use"
        },
        "testgenie.maxTokens": {
          "type": "number",
          "default": 4096,
          "description": "Maximum tokens for the response"
        },
        "testgenie.backendUrl": {
          "type": "string",
          "default": "http://127.0.0.1:8000",
          "description": "TestGenie backend URL"
        },
        "testgenie.jiraUrl": {
          "type": "string",
          "default": "",
          "description": "Your Jira Cloud URL e.g. https://your-domain.atlassian.net"
        },
        "testgenie.jiraEmail": {
          "type": "string",
          "default": "",
          "description": "Your Jira account email"
        },
        "testgenie.jiraToken": {
          "type": "string",
          "default": "",
          "description": "Your Jira API token from id.atlassian.com/manage-profile/security/api-tokens"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.109.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  },
  "dependencies": {
    "openai": "^6.22.0",
    "tree-sitter-java": "^0.23.5",
    "tree-sitter-javascript": "^0.25.0",
    "tree-sitter-python": "^0.25.0",
    "web-tree-sitter": "^0.26.5"
  }
}